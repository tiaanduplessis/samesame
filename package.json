{
  "name": "samesame",
  "version": "1.0.0",
  "description": "Simple, Lightweight type checking of multiple arguments",
  "license": "MIT",
  "node": "src/index.js",
  "browser": "lib/samesame.min.js",
  "engines": {
    "node": ">= 6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tiaanduplessis/samesame.git"
  },
  "homepage": "https://github.com/tiaanduplessis/samesame",
  "bugs": "https://github.com/tiaanduplessis/samesame/issues",
  "author": {
    "name": "Tiaan",
    "email": "tiaanduplessis@hotmail.com",
    "url": "tiaan.beer"
  },
  "scripts": {
    "start": "npm run build; npm run build:min",
    "build": "webpack",
    "build:min": "WEBPACK_ENV=production webpack -p",
    "dev": "webpack --progress --colors --watch",
    "pretest": "npm run lint",
    "test": "jest src/",
    "test:watch": "jest src/ --watch",
    "lint": "standard src/",
    "lint:fix": "npm run lint -- --fix",
    "coverage": "jest src/ --coverage",
    "precommit": "npm test"
  },
  "keywords": [
    "type",
    "typeof",
    "typechecker",
    "samesame"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "standard": "^10.0.2",
    "webpack": "^2.4.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "babel-plugin-add-module-exports"
    ]
  },
  "standard": {
    "ignore": [
      "lib/**"
    ]
  }
}
